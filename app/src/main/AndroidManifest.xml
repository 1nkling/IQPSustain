<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.SustainibilitySpotlight">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="The Sustainability Spotlight"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.SustainibilitySpotlight.WaterActivity"></activity>
        <activity android:name="com.example.SustainibilitySpotlight.DisplayMessageActivity" />
        <activity android:name="com.example.SustainibilitySpotlight.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.SustainibilitySpotlight.RecyclingActivity" />
        <activity android:name="com.example.SustainibilitySpotlight.SurveyActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.SustainibilitySpotlight.LearnMoreActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.SustainibilitySpotlight.TipsActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.SustainibilitySpotlight.GeneralActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.SustainibilitySpotlight.GeneralTipsActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.SustainibilitySpotlight.WaterTipsActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
